{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ShopeeDatabaseSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "EcommerceApiDb"
  },
  "ShopeeOAuth": {
    "PartnerId": "YOUR_PARTNER_ID",
    "PartnerKey": "YOUR_PARTNER_KEY",
    "RedirectUri": "https://yourdomain.com/api/account/oauth-callback"
  }
}
